# ln ~/.config/fish/conf.d/abbr.fish

# Quick which
abbr w which

# Docker 
abbr d 'docker'
abbr dc 'docker-compose'
abbr ds 'docker start'

# Git
abbr g 'git'
abbr gs 'git status'
abbr gf 'git fetch'
abbr gl 'git log --oneline --graph'
abbr gsw 'git switch'

# Conda
abbr ic 'init-conda'
abbr ca 'conda activate'

# kubectl
abbr k 'kubectl'
abbr kg 'kubectl get'

# Terraform
abbr tf 'terraform'

